                       147     ;hupe:
                       148     ;clr tr0; stop timer
                       149     ;clr P0.0
                       150     ;ret
                       151
                       152     ;----- Logik für die neue Generation -----
                       153     new_generation:
                       154     MOV R0,#0;
                       155     MOV R2,#0 ;Counter row starts with row 0
                       156
                       157     checkrow:
                       158     MOV R3,#0                       ;Counter column starts with column 0
                       159     MOV R4,#0                       ;R4 enthält die neuen Werte der Zellen der Zeile
                       160     MOV R1,#0;
                       161
                       162     checkcolumn:
                       163     MOV R7,#0
                       164     mov DPTR,#table2                ;R7 enthält die anzahl der nachbarn
                       165     ACALL checkTop
                       166     ACALL checkMid
                       167     ACALL checkBottom
                       168
                       169     mov DPTR,#table                 ;Setze DataPointer auf masken tabelle
                       170     mov A,R3
                       171     movC A,@A+DPTR                  ;Hole Maske für Spalte R3
                       172     mov R6,A;
                       173
                       174     MOV A,R2;
                       175     MOV R0,A;
                       176     MOVX A,@R0;                     ;Laden der entsprechenden Zeile
                       177     MOV R5,A;
                       178     ;MOV R1,#0
                       179
                       180     MOV A,R7;
                       181     CJNE A,#2,checkLessThan2        ;Wenn A != 2 überprüfe ob weniger
                       182     MOV A,R6                        ;Wenn A = 2 -> Behalte Wert der Zelle bei. Hole Maske
                       183     ANL A,R5                        ;Maskiere Wert der Zelle aus
                       184     ORL A,R1                        ;Berechne neue Zeile
                       185     MOV R1,A                        ;Füge Wert zur neuen Zeile hinzu
                       186     JMP checkEnd
                       187
                       188     checkLessThan2:
                       189     JNC checkMoreThan2              ;Wenn A < 2 -> C = 1
                       190     JMP checkEnd
                       191
                       192     checkMoreThan2:
                       193     CJNE A,#3,checkEnd              ;Wenn A != 3 muss die Zelle sterben
                       194     MOV A,R6                        ;Wenn A = 3 -> Setze Bit
                       195     ANL A,#11111111b                ;Maskiere Wert der Zelle aus
                       196     ORL A,R1                        ;Berechne neue Zeile
                       197     MOV R1,A                        ;Füge Wert zur neuen Zeile hinzu
                       198     JMP checkEnd
                       199
                       200     checkEnd:
                       201     ;A < 2 CLR
                       202     ;A = 3 SETB
                       203     ;A > 3 CLR
                       204
                       205     ;Repeat until column = 0
                       206     INC R3
                       207     MOV A,R3
                       208     CJNE A,#8,checkcolumn
                       209
                       210      ;setting new row from storage copy
                       211     ;movx @R0,TODO
                       212
                       213     ;Repeat until row = 0
                       214     MOV A,R2;
                       215     ADD A,#100;
                       216     MOV R0,A;
                       217     MOV A,R1;
                       218     MOVX @R0,A;
                       219
                       220     INC R2
                       221     MOV A,R2
                       222     CJNE A,#8,checkrow
                       223
                       224     call copyNewGenerationToField
                       225     ret
                       226
                       227
                       228     checkTop:
                       229     ;Lade Zähler der Zeile in Akku
                       230     MOV A,R2;
                       231     ;Wenn Akku gleich 0 muss zeile 7 betrachtet werden
                       232     MOV R5,#7;
                       233     JZ gotTopRow
                       234     ;Sonst wird die Aktuelle zeile gesetzt
                       235     DEC A;
                       236     MOV R5,A;
                       237     ;gotRow dient dazu, dass falls R2 = 0 R0 = 7 bleibt
                       238     gotTopRow:
                       239     ACALL calculateNeighbours
                       240     ret
                       241
                       242     checkMid:
                       243     ;Lade Zähler der Zeile in Akku
                       244     MOV A,R2;
                       245     MOV R5,A;
                       246     ACALL calculateMiddleNeighbours
                       247     ;DEC R7; ;NUR WENN MITTE GESEETZT
                       248     ret
                       249
                       250     checkBottom:
                       251     ;Lade Zähler der Zeile in Akku
                       252     MOV A,R2;
                       253     ;Wenn Akku gleich 7 muss zeile 0 betrachtet werden
                       254     MOV R5,#0;
                       255     CJNE A,#7,getBottomRow
                       256     JMP gotBottomrow
                       257     ;Sonst wird die Aktuelle zeile gesetzt
                       258     getBottomRow:
                       259     INC A;
                       260     MOV R5,A;
                       261     ;gotRow dient dazu, dass falls R2 = 7 R0 = 0 bleibt
                       262     gotBottomRow:
                       263     ACALL calculateNeighbours
                       264     ret
                       265
                       266     calculateNeighbours:
                       267     MOV A,R5;
                       268     MOV R0,A;
                       269
                       270     MOVX A,@R0;     ;Laden der entsprechenden Zeile
                       271     MOV R6,A        ;Speichern der rotierten Zeile
                       272
                       273     MOV A,R3                ;7-ZeilenNummer den benötigten rechts rotationen entspricht
                       274     MOVC A,@A+DPTR
                       275     ANL A,R6;
                       276     JZ neighbour1NotSet
                       277     INC R7;
                       278
                       279     neighbour1NotSet:
                       280     MOV A,R3                ;7-ZeilenNummer den benötigten rechts rotationen entspricht
                       281     INC A
                       282     MOVC A,@A+DPTR
                       283     ANL A,R6;
                       284     JZ neighbour2NotSet
                       285     INC R7;
                       286
                       287     neighbour2NotSet:
                       288     MOV A,R3                ;7-ZeilenNummer den benötigten rechts rotationen entspricht
                       289     INC A
                       290     INC A
                       291     MOVC A,@A+DPTR
                       292     ANL A,R6;
                       293     JZ neighbour3NotSet
                       294     INC R7;
                       295
                       296     neighbour3NotSet:
                       297     MOV R6,#0;
                       298     ret
                       299
                       300
                       301     calculateMiddleNeighbours:
                       302     MOV A,R5;
                       303     MOV R0,A;
                       304     MOVX A,@R0;     ;Laden der entsprechenden Zeile
                       305     MOV R6,A        ;Speichern der rotierten Zeile
                       306     ;Berechnen um wie viele Stellen die Zeile nach rechts verschoben werden muss
                       307
                       308     ;MOV R4,A       ;Speichern des zählers
                       309
                       310
                       311     ;Rotieren der Zeile bis der zähler R4 null ist
                       312     ;RL A           ;einmalig links rotieren da erst nach der rechts rotation auf 0 geprüft wird
                       313     ;RL A           ;einmalig links rotieren da erst nach der rechts rotation auf 0 geprüft wird
                       314     ;rotateRight2:  ;so wird verhindert dass wenn der zähler 0 ist eine rotation stattfindet
                       315     ;RR A;
                       316     ;DJNZ R4,rotateRight2
                       317
                       318     MOV A,R3                ;7-ZeilenNummer den benötigten rechts rotationen entspricht
                       319     MOVC A,@A+DPTR
                       320     ANL A,R6;
                       321     JZ neighbour2NotSet2
                       322     INC R7;
                       323
                       324     neighbour2NotSet2:
                       325     MOV A,R3                ;7-ZeilenNummer den benötigten rechts rotationen entspricht
                       326     INC A
                       327     INC A
                       328     MOVC A,@A+DPTR
                       329     ANL A,R6;
                       330     JZ neighbour3NotSet2
                       331     INC R7;
                       332
                       333     neighbour3NotSet2:
                       334     MOV R6,#0;
                       335     ret
                       336
